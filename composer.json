{
    "name": "kijtra/textdiff",
    "description": "Simple text diff Class. 簡易的なテキスト比較ライブラリです。",
    "license": "MIT",
    "authors": [
        {
            "name": "Kijtra",
            "email": "kijtra@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Kijtra\\TextDiff\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kijtra\\TextDiff\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
